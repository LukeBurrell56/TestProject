  <Calculations>
	<Calculation>
	  <Part1>TotalAttack = Attack*2 + Lvl + Agil/10 + Int/10 + Misc</Part1>
	  <Part2>TotalEvasion = Evasion + 1/4Lvl + 1/2(Agil/10 + Int/10) + Misc</Part2>
	  <Part3>TA+TD = Percentage</Part3>
	</Calculation>
	<Calculation>
	  <Part1>Percent to hit = (TotalAttack / Percentage) x 100</Part1>
	  <Part2>Percent to evade = (TotalEvasion / Percentage) x 100</Part2>
	  <Part3>TotalHP = HP + Lvl + Might/10</Part3>
	</Calculation>
	<Calculation>
	  <Part1>TotalDamage = Damage + lvl + Might/10 + Misc (Ranged is (Might/10+Agil/10)1/2</Part1>
	  <Part2>TotalDefence = Defence + 1/2lvl + 1/2(Might/10 + Will/10) + Misc</Part2>
	  <Part3>TotalDamage - TDefence = DamageDealt min 10</Part3>
	</Calculation>
	<Calculation>
	  <Part1>TotalDamage = Damage + lvl + Will/10 + Misc</Part1>
	  <Part2>TotalResistance = 1/2Resistance + 1/4lvl + 1/2(Int/10 + Will/10) + Misc</Part2>
	  <Part3>TotalDamage - TotalResistance = DamageDealt min 10</Part3>
	</Calculation>
	<Calculation>
	  <Part1>CritRange = 1/2(Agil/10)</Part1>
	  <Part2>Crit = DamageDealt * 1.2</Part2>
	  <Part3>Crit+Bonus DamageDealt * 1.44</Part3>
	</Calculation>
	<Calculation>
	  <Part1>Bonus TAtt x2, DamageDealt x1.2, TEvax1.2, CritRange x3</Part1>
	  <Part2>Bonusx2 TAtt x4, DamageDealt x1.44, TEvax1.44, CritRange x9</Part2>
	  <Part3>Crit+Bonusx2 DamageDealt x1.728</Part3>
	</Calculation>
  </Calculations>
